command /freeze [<offline player>]:
    permission: freezemenu.open, punishmenu.*
    permission message: {@Prefix} {@NoPerms}
    trigger:
        if arg 1 is set:
            if arg 1 is online:
                set {freeze.move.%arg-1%} to true
                send "&c%arg-1% &7has been frozen."
                message "&7You have been frozen by &c%player%&7." to arg 1
            else:
                message "{@Prefix} {@NoPlayerConnect}"

command /unfreeze [<offline player>]:
    permission: freezemenu.open, punishmenu.*
    permission message: {@Prefix} {@NoPerms}
    trigger:
        if arg 1 is set:
            if arg 1 is online:
                set {freeze.move.%arg-1%} to false
                send "&c%arg-1% &7has been unfrozen."
                message "&7You have been unfrozen by &c%player%&7." to arg 1
            else:
                message "{@Prefix} {@NoPlayerConnect}"

on any movement:
    if {freeze.move.%player%} is true:
        message "&cYou are frozen !"
        cancel event

