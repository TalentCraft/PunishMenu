command /punish [<text>] [<text>]:
    aliases: p
    permission: punishmenu.*
    permission message: {@Prefix} {@NoPerms}
    trigger:
        if arg-1 is not set:
            message "{@Prefix} &4&lCorrect usage: &2&l/punish help (&c&lPages Available: &2&l1)"
            message "{@Prefix} &4&lCorrect usage: &2&l/punish info (&c&lPages Available: &2&l1)"
        if arg-1 is "help":
            if player has permission "punishhelp.use":    
                if arg-2 is "1":
                    message ""
                    message ""
                    message ""
                    message ""
                    message ""
                    message "&b&l&m----------------------------------------"
                    message "&c&l                 Page 1/1."
                    message "&b&l&m----------------------------------------"
                    message ""
                    message "&c - /punishmenu <player> - Open the menu main is skript. - punishmenu.open"
                    message "&c - /punish help (1) - List of commands/permissions. - punishhelp.use"
                    message "&c - /punish info (1) - See skript information. - punishinfo.use"
                    message ""
                    message "&b&l&m----------------------------------------"
                    message "&c&l                 Page 1/1."
                    message "&b&l&m----------------------------------------"
        if arg-1 is "info":
            if player has permission "punishinfo.use":  
                if arg-2 is "1":
                    message ""
                    message ""
                    message "&b&l&m----------------------------------------"
                    message "&c&l                 Page 1/1."
                    message "&b&l&m----------------------------------------"
                    message ""
                    message "{@Prefix} Skript developed by alexisl315"
                    message "{@Prefix} The skript provides facilites added and punishments."
                    message "{@Prefix} Version >> 21.2.0"
                    message ""
                    message "{@Prefix} Site (Spigot) -> https://www.spigotmc.org/resources/punishmenu.23209/"
                    message ""
                    message "&b&l&m----------------------------------------"
                    message "&c&l                 Page 1/1."
                    message "&b&l&m----------------------------------------"

command /punishmenu [<offline player>]:
    aliases: pmenu, pm
    permission: punishmenu.open, punishmenu.*
    permission message: {@Prefix} {@NoPerms}
    trigger:
        if arg 1 is set:
            if arg 1 is online:
                wait 2 ticks
                open chest with 6 rows named "{@MenuNameMain}" to player
                wait 4 ticks
                format slot 0 of player with charcoal named "&f&lKick" to close then run [kick arg 1 due to "{@KickReason}"]
                format slot 2 of player with gold bar named "&f&lMute" to close then run [make player execute command "mutemenu %arg 1%"]
                format slot 4 of player with redstone named "&f&lKill" to close then run [make player execute command "kill %arg 1%"]
                format slot 6 of player with diamond named "&f&lBan" to close then run [make player execute command "banmenu %arg 1%"]
                format slot 8 of player with emerald named "&f&lClear inventory" to close then run [make player execute command "clear %arg 1%"]
                format slot 10 of player with blaze rod named "&f&lLightning" to close then run [make player execute command "lightning %arg 1%"]
                format slot 12 of player with water bottle named "&f&lEffects" to close then run [make player execute command "effectsmenu %arg 1%"]
                format slot 14 of player with eye of ender named "&f&lGamemodes" to close then run [make player execute command "gamemodemenu %arg 1%"]
                format slot 16 of player with flint and steel named "&f&lBurn" to close then run [make player execute command "burnmenu %arg 1%"]
                format slot 18 of player with water bucket named "&f&lFreeze" to close then run [make player execute command "freezemenu %arg 1%"]
                format slot 20 of player with tnt named "&f&lTraps" to close then run [make player execute command "trapsversionsmenu %arg 1%"]
                format slot 22 of player with name tag named "&f&lRename" to close then run [make player execute command "namesmenu %arg 1%"]
                format slot 24 of player with torch named "&f&lTimes" to close then run [make player execute command "timesmenu %arg 1%"]
                format slot 26 of player with ender pearl named "&f&lTeleport" to close then run [make player execute command "teleportmenu %arg 1%"]
                format slot 36 of player with bars named "&6&lFence" to be unstealable
                format slot 37 of player with bars named "&6&lFence" to be unstealable
                format slot 38 of player with bars named "&6&lFence" to be unstealable
                format slot 39 of player with bars named "&6&lFence" to be unstealable
                format slot 40 of player with bars named "&6&lFence" to be unstealable
                format slot 41 of player with bars named "&6&lFence" to be unstealable
                format slot 42 of player with bars named "&6&lFence" to be unstealable
                format slot 43 of player with bars named "&6&lFence" to be unstealable
                format slot 44 of player with bars named "&6&lFence" to be unstealable
                format slot 45 of player with ender pearl named "&c&lClose the menu !" to close
                format slot 49 of player with paper named "&3&lPage 1/2" to be unstealable
                format slot 50 of player with arrow named "&f&lPage 2 &2&l>>" to close then run [make player execute command "menu2punishmenu %arg 1%"]
                format slot 53 of player with blaze powder named "&3&lOptions" to close then run [make player execute command "optionsmenu %arg 1%"]
            else:
                message "{@Prefix} {@NoPlayerConnect}"
        else:
            message "{@Prefix} &4&lCorrect usage: &2&l/punishmenu <player>"

